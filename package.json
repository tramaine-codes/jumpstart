{
  "name": "@triviumsoftware/jumpstart",
  "type": "module",
  "version": "0.0.0",
  "description": "Jumpstart is a small, simple project generator based on my personal preferences.",
  "author": "Tramaine L. Gillus",
  "repository": {
    "type": "git",
    "url": "git@github.com:tgillus/jumpstart.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=18",
    "npm": ">=10"
  },
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "/lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run clean && npm run barrels && npm test && tsc",
    "build:ci": "npm run clean && npm test && tsc",
    "clean": "del ./lib ./coverage",
    "format": "prettier . --write",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "prepack": "npm run build",
    "prepare": "husky",
    "release": "release-it --ci",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "test:watch": "vitest",
    "watch": "tsc -c"
  },
  "dependencies": {
    "liquidjs": "10.21.0",
    "pkg-dir": "8.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.0",
    "@commitlint/config-conventional": "19.8.0",
    "@eslint/js": "9.24.0",
    "@faker-js/faker": "9.6.0",
    "@release-it/conventional-changelog": "10.0.0",
    "@types/node": "22.14.0",
    "@vitest/coverage-v8": "3.1.1",
    "del-cli": "6.0.0",
    "eslint": "9.24.0",
    "globals": "16.0.0",
    "husky": "9.1.7",
    "lint-staged": "15.5.0",
    "prettier": "3.5.3",
    "release-it": "18.1.2",
    "testdouble": "3.20.2",
    "tsx": "4.19.3",
    "typescript": "5.8.3",
    "typescript-eslint": "8.29.1",
    "vitest": "3.1.1"
  }
}
